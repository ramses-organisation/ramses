&RUN_PARAMS
hydro=.true.
rt=.false.
nrestart=0
nremap=10
poisson=.true.
pic=.true.
sink=.true.
nsubcycle=1,2
verbose=.false.
/

&INIT_PARAMS
nregion=1
region_type(1)='square'
x_center=0.5
y_center=0.5
z_center=0.5
length_x=10.0
length_y=10.0
length_z=10.0
exp_region=10.0
d_region=1d-2		   	  ! 1e-2 hydrogen atoms per cc
p_region=8.69869d-9       ! 1000 K
/

&AMR_PARAMS
levelmin=6
levelmax=7
ngridmax=350000
npartmax=2200
nexpand=4,4,4
boxlen=1. ! kpc
/

&OUTPUT_PARAMS
foutput=1
tend=0.05  ! 0.05 Myr
/

&HYDRO_PARAMS
gamma=1.666666667
courant_factor=0.8
riemann='hllc'
slope_type=1
pressure_fix=.true.
beta_fix=0.5
/

&COOLING_PARAMS
cooling=.false.
metal=.false.
z_ave=1.0
/

&UNITS_PARAMS
units_density = 1.66d-24 ! 1 H/cc
units_time    = 3.1556926d13 ! 1 Myr
units_length  = 0.308567758128200E+22 ! 1 kpc
/

&REFINE_PARAMS
m_refine=8,8,8
jeans_refine=4,4,4
mass_sph=1e8
interpol_type=0
interpol_var=1
sink_refine=.true.
/

&BOUNDARY_PARAMS
nboundary = 6
no_inflow=.true.
bound_type= 2, 2, 2, 2, 2, 2
ibound_min=-1,+1,-1,-1,-1,-1
ibound_max=-1,+1,+1,+1,+1,+1
jbound_min= 0, 0,-1,+1,-1,-1
jbound_max= 0, 0,-1,+1,+1,+1
kbound_min= 0, 0, 0, 0,-1,+1
kbound_max= 0, 0, 0, 0,-1,+1
/

&POISSON_PARAMS
gravity_type=0
epsilon=1.0d-4
cg_levelmin=999
cic_levelmax=14
/

&FEEDBACK_PARAMS
eta_sn=1.0e-10
yield=0.1
f_w=0.
delayed_cooling=.false.
/

&SF_PARAMS
m_star=1.0
n_star=3.e5
T2_star=10.
g_star=1.0  ! 1.0 gives flat relation
eps_star=1e-10
/

&SINK_PARAMS
smbh=.true.
agn=.false.
create_sinks=.false.
nsinkmax=1
mass_sink_seed=1.0d6 ! in M_sun
mass_smbh_seed=0
mass_sink_direct_force=1.0 ! in M_sun
accretion_scheme='bondi'
eddington_limit=.true.
acc_sink_boost=1.0 ! 1 is no boost
T2_min=0 ! always blast
AGN_fbk_frac_ener=0.0
AGN_fbk_frac_mom=0.0
AGN_fbk_mode_switch_threshold=0.0
/
